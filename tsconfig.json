{
    "compilerOptions": {
        "target": "es5",
        "module":"commonjs",
        "noImplicitAny": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "declaration": true,
        "outDir": "compiled",
        "lib": [
            "es5"
        ]
    },
    "include": [
        "./src/**/*.ts"
    ],
    "exclude": [
        "./src/**/test-*.ts",
        "./src/**/testLib.ts"
    ]
}